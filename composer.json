{
  "name": "unofficialmc2/synchr-ntp",
  "description": "Surcharge des fonctions de base de PHP avec une synchronisation \u00e0 un service ntp",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Fabien Sanchez",
      "email": "fabien.sanchez@arche-mc2.fr"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.6",
    "phpstan/phpstan": "^0.12.88",
    "ocramius/package-versions": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "SynchroNtp\\": "./src"
    },
    "files": [
      "./src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "./tests",
      "SynchroNtp\\": "./tests/SynchroNtp"
    }
  }
}
